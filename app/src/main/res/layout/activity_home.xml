<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/white"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".home">


    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:id="@+id/linearLayout"
    android:weightSum="2">
    <FrameLayout
        android:layout_height="wrap_content"
    android:background="@color/teal"
    android:padding="@dimen/activity_horizontal_margin"
        android:layout_marginRight="5dp"
    android:id="@+id/htip"
    android:layout_alignLeft="@+id/reminder"
    android:layout_alignStart="@+id/reminder"
        android:layout_weight="1"
        android:layout_width="wrap_content">

        <TextView
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  Health       \n  Tips"
            android:id="@+id/healthtips"
            android:textSize="25sp"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
</FrameLayout>


    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/teal"
        android:padding="@dimen/activity_horizontal_margin"
        android:layout_above="@+id/reminder"
        android:layout_alignRight="@+id/reminder"
        android:layout_marginLeft="5dp"
        android:layout_alignEnd="@+id/reminder"
        android:layout_weight="1"
        android:id="@+id/healthn"
        android:onClick="healthnews"
       >

        <TextView
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  Health       \n  News"
            android:id="@+id/healthnews"
            android:textSize="25sp"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </FrameLayout>
</LinearLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:background="@color/teal"
        android:padding="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_horizontal_margin"
        android:id="@+id/remiander"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"

        android:layout_below="@id/linearLayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:text="Reminder"
            android:id="@+id/reminder"
            android:textSize="25sp"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/orange"
        android:layout_marginTop="@dimen/activity_horizontal_margin"
        android:id="@+id/frameLayout6">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Copyright Â© 2015 "
            android:padding="5dp"
            android:textSize="20sp"
            android:textColor="@color/white"
            android:layout_gravity="center" />
    </FrameLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/remiander"
        android:layout_above="@id/frameLayout6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout2">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/teal"
        android:layout_marginTop="@dimen/activity_horizontal_margin"
        android:padding="@dimen/activity_horizontal_margin"
        android:id="@+id/frameLayout4"
        android:onClick="maps"
        android:layout_weight="1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Find   \n Hospitals"
            android:id="@+id/hospital"
            android:textSize="25sp"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/teal"
        android:padding="@dimen/activity_horizontal_margin"
        android:id="@+id/frameLayout5"
        android:layout_marginTop="@dimen/activity_horizontal_margin"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/healthn"
        android:layout_alignEnd="@+id/healthn"
        android:onClick="about"
        android:layout_weight="1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" About      \n Us"
            android:id="@+id/about"
            android:textSize="25sp"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </FrameLayout>

</LinearLayout>
  <FrameLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="@color/red"
      android:layout_alignTop="@+id/linearLayout2"
      android:layout_alignBottom="@+id/linearLayout2"
      android:layout_alignParentRight="true"
      android:layout_toRightOf="@id/linearLayout2"
      android:layout_marginLeft="@dimen/activity_horizontal_margin"
      android:layout_marginTop="@dimen/activity_horizontal_margin"
      android:layout_alignParentEnd="true">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/bg"
        android:layout_alignTop="@+id/linearLayout2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/bg" />
  </FrameLayout>



















    <FrameLayout
        android:layout_width="match_parent"
        android:id="@+id/before"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:background="@color/white"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="95dp"
            android:layout_height="286dp"
            android:id="@+id/hint"
            android:textSize="15sp"

            android:layout_gravity="left|center_vertical"
            android:text="nothing"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:id="@+id/textView"
            android:gravity="center"
            android:text="@string/name"
            android:textSize="30sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_gravity="center_horizontal|top"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="400dp"
            android:layout_height="300dp"
            android:id="@+id/imageView"
            android:src="@drawable/iconpn"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_gravity="left|center_vertical" />

    </FrameLayout>





















</RelativeLayout>
